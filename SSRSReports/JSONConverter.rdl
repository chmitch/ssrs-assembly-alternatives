  <Code>Public Function JSONConverter(ByVal text As String, ByVal host as String, ByVal authKey As String) As String
    Dim responseString As String

 

    Try
        Dim path As String
        path = "/api/JSONConverter?code=" + authKey

 

        '' This API accepts query string pararameters.
        Dim apiParams As String
        apiParams = "&amp;json=" + text 

 

        Dim uri As String
        uri = host + path + apiParams 

 

        '' Create request
        Dim webRequest As Net.WebRequest
         webRequest = webRequest.Create(uri)

 

        webRequest.Method = "Get"
         webRequest.ContentType = "application/json"

' responseString = uri



        '' Execute translate request
        Dim response As Net.WebResponse
        response = webRequest.GetResponse()

 

        Using responseStream As IO.Stream = response.GetResponseStream()
            Dim responseReader As IO.StreamReader = New IO.StreamReader(responseStream)
            responseString = responseReader.ReadToEnd()
        End Using

    Catch e As Exception
      '  responseString = e.Message
	responseString = "no string returned|no string returned|no string returned"
    Finally
            JSONConverter = responseString
    End Try
    End Function
</Code>
